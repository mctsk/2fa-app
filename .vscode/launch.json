{
    "version": "0.2.0",
    "configurations": [
      {
        "name": "(Windows) Launch",
        "type": "cppvsdbg",
        "request": "launch",
        "program": "${workspaceFolder}\\out\\2fa-app-win32-x64\\2fa-app.exe",
        "args": ["your-electron-project-path"],
        "stopAtEntry": false,
        "cwd": "${workspaceFolder}",
        "environment": [
            {"name": "ELECTRON_ENABLE_LOGGING", "value": "true"},
            {"name": "ELECTRON_ENABLE_STACK_DUMPING", "value": "true"},
            {"name": "ELECTRON_RUN_AS_NODE", "value": ""},
        ],
        "externalConsole": false,
        "sourceFileMap": {
            "o:\\": "${workspaceFolder}",
        },
      },
    ]
  }